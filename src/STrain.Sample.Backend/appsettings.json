{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace"
    },
    "Loki": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Trace",
        "Microsoft.Hosting.Lifetime": "Trace"
      },
      "Formatter": "Json",
      "Client": "Http",
      "Http": {
        "Address": "https://loki.mihben.work"
      },
      "DynamicLabels": {
        "IncludeCategory": true,
        "IncludeLogLevel": true,
        "IncludeException": true,
        "IncludeEventId": true
      },
      "StaticLabels": {
        "AdditionalStaticLabels": {
          "Application": "STrain.Sample.Backend",
          "Environment": "Develop"
        }
      }
    }
  },
  "Senders": {
    "Internal": {
      "BaseAddress": "http://localhost:5100/",
      "Path": "api"
    }
  }
}
